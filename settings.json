{
    "autosu": true,
    "basename": true,
    "colorscheme": "one",
    "comment": false,
    "diffgutter": true,
    "divreverse": false,
    "hlsearch": true,
    "linter": false,
    "literate": false,
    "lsp.server": "python=python -OO -m pylsp",
    "mkparents": true,
    "permbackup": true,
    "rmtrailingws": true,
    "savecursor": true,
    "saveundo": true,
    "scrollmargin": 10,
    "scrollspeed": 1,
    "softwrap": true,
    "statusformatl": " $(modified) $(filename)   $(status.size)   $(status.branch) ",
    "statusformatr": " $(opt:encoding)    $(opt:fileformat)  $(opt:filetype)    $(status.lines)  $(percentage)%  $(line):$(col) ",
    "tabmovement": true,
    "tabstospaces": true,
    "wordwrap": true
}
